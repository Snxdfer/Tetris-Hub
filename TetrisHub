    Text = "made by: elrandom#1311", 
    
    Button1 = "Thanks!",
    Duration = 20 
    })
    
    
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Tetris Hub", "Serpent")

    local Tab = Window:NewTab("Home")
    local Section = Tab:NewSection("Information")

    Section:NewLabel("Username: "..Player.Name..".")

    Section:NewLabel("GamePlace ID: "..game.PlaceId.."")

    Section:NewLabel("Thanks for using my scripthub!")

    Section:NewButton("Copy discord", "my discord!", function()
        setclipboard("elrandom#1311")
    end)


    local LaundryTab = Window:NewTab("Laundry Simulator")
